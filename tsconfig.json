{
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "sourceMap": true,
        "jsx": "react",
        "moduleResolution": "node",
        "noResolve": false,
        "removeComments": true,
        "allowJs": true,
        "types": [
            "lodash",
            "react",
            "react-dom"
          ],
        "noImplicitAny": false,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "noEmit": false,
        "strict": false,
        "importHelpers": true,
        "declaration": true,
        "rootDir": "./",
        "outDir": "./",
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "strictPropertyInitialization": true,
        "noImplicitThis": false,
        "alwaysStrict": true,
        "noUnusedLocals": false,
        "downlevelIteration": true,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "assets/*"
            ],
            "@api/*": [
                "src/api/*"
            ],
            "@const/*": [
                "src/const/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@css/*": [
                "src/css/*"
            ],
            "@dataLocal/*": [
                "src/dataLocal/*"
            ],
            "@element/*": [
                "src/element/*"
            ],
            "@HOC/*": [
                "src/HOC/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@images/*": [
                "src/images/*"
            ],
            "@language/*": [
                "src/language/*"
            ],
            "@libJS/*": [
                "src/libJS/*"
            ],
            "@logEventSystem/*": [
                "src/logEventSystem/*"
            ],
            "@navigation/*": [
                "src/navigation/*"
            ],
            "@permissions/*": [
                "src/permissions/*"
            ],
            "@redux/*": [
                "src/redux/*"
            ],
            "@resources/*": [
                "src/resources/*"
            ],
            "@screen/*": [
                "src/screen/*"
            ],
            "@security/*": [
                "src/security/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@util/*": [
                "src/util/*"
            ],
            "*": [
                "./*",
                "node_modules/*"
            ],
        }
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "./node_modules/**/*",
        "build",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js",
    ],
    "typeRoots": [
        "../@types",
        "./node_modules/@types/"
      ]
}